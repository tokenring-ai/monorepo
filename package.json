{
  "name": "@tokenring-ai/monorepo",
  "description": "TokenRing Monorepo with all community apps and packages",
  "version": "0.2.13",
  "devDependencies": {
    "@tokenring-ai/chat-frontend": "workspace:*",
    "@tokenring-ai/coder": "workspace:*",
    "@tokenring-ai/web-terminal": "workspace:*",
    "@tokenring-ai/writer": "workspace:*",
    "@types/bun": "^1.3.8",
    "bun-types": "^1.3.8",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit",
    "coder": "bun --env app/coder/src/tr-coder.js",
    "writer": "bun --env app/writer/src/tr-writer.js",
    "test": "bun run test",
    "prepare": "husky",
    "update-all-dependencies-latest": "npx syncpack@alpha update"
  },
  "type": "module",
  "workspaces": {
    "packages": [
      "pkg/*",
      "app/*",
      "frontend/*",
      "deps/*",
      "docs"
    ]
  },
  "dependencies": {
    "@types/mime-types": "^3.0.1",
    "mime-types": "^3.0.2"
  }
}
